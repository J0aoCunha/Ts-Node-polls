generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Poll {
  id         String   @id @unique() @default(cuid())
  title      String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())

  @@map("poll")
}
